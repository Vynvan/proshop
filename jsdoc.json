{
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "allowUnknownTags": true,
    "template": "./node_modules/better-docs"
  },
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/|\\.git/)"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "theme": "slate"
  }
}